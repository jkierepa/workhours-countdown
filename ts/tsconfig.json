{
    "compilerOptions":{
        "module": "commonjs",
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "target": "ES2017",
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": "./",
        "incremental": true,
        "allowUnreachableCode":true,
        "allowUnusedLabels":true,
        "noImplicitAny":true,
        "noImplicitReturns":false,
        "noImplicitUseStrict":false
    },
    "compileOnSave":true
}